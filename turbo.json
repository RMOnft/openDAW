{
  "//": "Turbo configuration orchestrates tasks across packages. Versioning policy: see packages/docs/docs-dev/build-and-run/versioning.md",
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "@opendaw/studio-forge-boxes#build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*", "src/**"],
      "outputs": ["dist/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "docs:build": {
      "dependsOn": ["^build"],
      "outputs": ["packages/docs/build/**"]
    }
  }
}
